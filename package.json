{
  "name": "tournaments",
  "version": "1.0.50",
  "scripts": {
    "test:e2e": "vitest --config vitest.node.config.ts",
    "test:e2e:ui": "vitest --config vitest.node.config.ts --ui",
    "test:e2e:coverage": "vitest --config vitest.node.config.ts --coverage",
    "dev": "bun run src/index.ts",
    "migrate:deploy": "npx prisma migrate deploy",
    "prisma:generate": "bunx prisma@6.19.0 generate",
    "db:seed": "npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/eden": "^1.4.6",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/client": "6.19.0",
    "elysia": "latest",
    "elysia-rate-limit": "^4.5.0",
    "pg": "^8.18.0",
    "pino": "^10.3.0"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.11.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "4.0.10",
    "@vitest/ui": "^4.0.18",
    "bun-types": "latest",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18"
  },
  "module": "src/index.js"
}